<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">

	<!-- static content 
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/js/**</from>
		<to>/js/$1</to>
	</rule>
	<rule>
		<from>/css/**</from>
		<to>/css/$1</to>
	</rule>
	<rule>
		<from>/html/**</from>
		<to>/html/$1</to>
	</rule>
	-->
	
	<!-- dispatcher mapping dispose -->
	<!-- nie dziala na flow controllera --> 
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
	
</urlrewrite>